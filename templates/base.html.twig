<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Studi Bonn Task{% endblock title %}</title>
        <link rel = "stylesheet" href="{{ asset('css/styles.css') }}">
        {% block stylesheets %}
        {% endblock %}

    </head>
    <body>
        
        
        {% block addButton %}
            <button type="button" >
                <img src="{{ asset('images/add.svg') }}" alt="Add" style="width: 40px; height: 40px;" >
            </button>    
        {% endblock addButton %}
        
        {% block logoutButton %}
        <a href="{{ path('app_logout') }}">
            <button type="button">
                    <img src="{{ asset('images/logout.svg') }}" alt="Logout" style="width: 40px; height: 40px;">
                </button> 
        </a>       
        {% endblock logoutButton %}
        
        <br>

        {% block body %}
        {% endblock body %}
    </body>
</html>
